#!/bin/sh
# postinst script for php5-phalcon
#
# see: dh_installdeb(1)

set -e

case "$1" in
    configure)
	php5enmod phalcon
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
