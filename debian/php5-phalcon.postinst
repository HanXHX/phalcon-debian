#!/bin/sh
# postinst script for php5-phalcon
#
# see: dh_installdeb(1)

set -e

dpkg-maintscript-helper mv_conffile /etc/php5/conf.d/phalcon.ini \
	/etc/php5/mods-available/phalcon.ini 1.3.0-1~ -- "$@"

case "$1" in
    configure)
		php5enmod phalcon
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
